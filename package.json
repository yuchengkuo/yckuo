{
  "name": "yckuo",
  "description": "YuCheng Kuo's home page on the web.",
  "authors": "YuCheng Kuo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchengkuo/yckuo"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:content": "velite dev",
    "dev:svelte": "vite dev",
    "build": "run-s build:*",
    "build:content": "velite build",
    "build:svelte": "vite build",
    "format": "prettier --write .",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "vercel-install": "bash scripts/vercel-install.sh"
  },
  "type": "module",
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@iconify-json/ri": "^1.2.6",
    "@iconify-json/tabler": "^1.2.23",
    "@markdoc/markdoc": "^0.4.0",
    "@radix-ui/colors": "^3.0.0",
    "@shikijs/transformers": "^1.29.2",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/node": "^22.19.0",
    "cloudinary": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "shiki": "^1.29.2",
    "svelte": "^5.43.4",
    "svelte-check": "^4.3.3",
    "svelte-seo": "^1.7.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "unocss": "^66.5.4",
    "velite": "0.2.4",
    "vite": "^5.4.21"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "cloudinary-build-url": "^0.2.4",
    "motion": "^12.23.24",
    "sveltejs-markdoc": "^0.2.0"
  }
}
